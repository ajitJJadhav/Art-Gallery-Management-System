<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Some Website</title>
    </head>
    <body>
        <% include partials/header.ejs %>
        <p>Customer Query Result</p>
            <table class="table">
                <tr>
                    <th>Customer Name</th>
                    <th>Money Spent</th>
                    <th>Customer ID</th>
                </tr>
                <% Object.values(result).forEach(res => { %>
                    <tr>
                        <td><%= res.name %></td>
                        <td><%= res.moneySpent %></td>
                        <td><%= res.custid %></td>
                    </tr>
                <% }) %>    
            </table>
        <% include partials/footer.ejs %>
    </body>
</html>
